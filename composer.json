{
    "name": "terminal42/mage-tools",
    "description": "Magephp tools for easier deployment of Contao and Symfony systems.",
    "keywords": [
        "mage",
        "magephp",
        "contao",
        "symfony",
        "deployment"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "http://terminal42.ch"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "andres-montanez/magallanes": "^3.1"
    },
    "conflict": {
        "smart-core/accelerator-cache-bundle": "<1.2 || >=2.0"
    },
    "suggest": {
        "smart-core/accelerator-cache-bundle": "To clear OpCode and other caches using AcceleratorCacheClearTask"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\MageTools\\": "src"
        }
    },
    "bin": ["bin/mage-deploy-all"]
}
